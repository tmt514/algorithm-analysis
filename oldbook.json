{
  "title": "演算法的分析與證明",
  "plugins": [
    "katex-plus-macro",
    "prism",
    "-highlight",
    "ga"
  ],
  "pluginsConfig": {
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "ga": {
      "token": "UA-68887724-4"
    },
    "katex-plus-macro": {
      "macros": {
        "\\E": "\\mathbb{E}",
        "\\Inv": "\\mathrm{Inv}",
        "\\floor": "\\left\\lfloor{#1}\\right\\rfloor",
        "\\ceil": "\\left\\lceil{#1}\\right\\rceil"
      }
    }
  },
  "styles": {
    "website": "styles/website.css"
  }
}